msgstr "Flujos Bloques  Comprimido Sin-Comprimir Relac  Verif   Nombre-Fichero"

#: src/xz/list.c:717
#, c-format
msgid "  Streams:            %s\n"
msgstr "  Flujos:               %s\n"

#: src/xz/list.c:719
#, c-format
msgid "  Blocks:             %s\n"
msgstr "  Bloques:              %s\n"

#: src/xz/list.c:721
#, c-format
